{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Deny",
        "Action": "sts:AssumeRoleWithWebIdentity",
        "Principal": "*",
        "Resource": "*",
        "Condition": {
          "StringNotEqualsIfExists": {
            "login.salesforce.com:aud": "<my-tenant-value>"
          },
          "StringNotLikeIfExists": {
            "oidc.examplecorp.com:aud": "*"
          }
        }
      }
    ]
  }